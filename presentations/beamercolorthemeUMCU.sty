\ProvidesPackage{beamercolorthemeUMCU}[2016/06/14]

\DeclareOptionBeamer{compress}{\beamer@compresstrue}
\ProcessOptionsBeamer

\mode<presentation>

\definecolor{beamer@umcublue}{rgb}{0.0,0.324,0.617}

\useoutertheme[footline=authortitle]{miniframes}
\useinnertheme{circles}


%\usecolortheme{whale}
\setbeamercolor*{palette primary}{use=structure,fg=white,bg=beamer@umcublue}
\setbeamercolor*{palette secondary}{use=structure,fg=white}
\setbeamercolor*{palette tertiary}{use=structure,fg=white}
\setbeamercolor*{palette quaternary}{fg=white,bg=black}
\setbeamercolor*{titlelike}{parent=palette primary}

\setbeamercolor*{separation line}{}
\setbeamercolor*{fine separation line}{}

\usecolortheme{orchid}

\setbeamercolor{structure}{fg=beamer@umcublue}
\setbeamercolor{titlelike}{parent=structure}
\setbeamercolor{frametitle}{fg=beamer@umcublue}
\setbeamercolor{title}{fg=white}
\setbeamercolor{author}{fg=white}
\setbeamercolor{date}{fg=white}
\setbeamercolor{item}{fg=beamer@umcublue}

\mode
<all>


