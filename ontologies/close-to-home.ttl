@prefix : <http://localhost:8890/TestGraph#> .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix edam: <http://edamontology.org/> .



##
## Ontology metadata
## ----------------------------------------------------------------------------
<http://localhost:8890/TestGraph> rdf:type owl:Ontology ;
  owl:versionIRI <http://localhost:8890/TestGraph/0.0.1> ;
  <http://purl.org/dc/elements/1.1/creator> "Roel Janssen" ;
  <http://purl.org/dc/elements/1.1/title> "Ontology for describing variants" .



## Sample Class
## ----------------------------------------------------------------------------
:Sample a owl:Class .

  # Properties
  # -------------------------------------------------------------------------
  :name a owl:DatatypeProperty ;
  rdfs:label "Name" ;
  rdfs:comment "The name of a sample." ;
  rdfs:domain :Sample ;
  rdfs:range xsd:string .

  :caller a owl:DatatypeProperty ;
  rdfs:label "Caller" ;
  rdfs:comment "The caller of the sample" ;
  rdfs:range xsd:string .

  # Scoping
  # -------------------------------------------------------------------------
  :Sample rdfs:domain :Variant .



## GenomePosition class
## ----------------------------------------------------------------------------
:GenomePosition a owl:Class .

  # Properties
  # -------------------------------------------------------------------------
  :chromosome a owl:DatatypeProperty ;
    rdfs:label   "Chromosome" ;
    rdfs:comment "The CHR field of the VCF file." ;
    rdfs:domain  :GenomePosition ;
    rdfs:range   xsd:string .

  :position a owl:DatatypeProperty ;
    rdfs:label   "Start position" ;
    rdfs:comment "The POS field of the VCF file." ;
    rdfs:domain  :GenomePosition ;
    rdfs:range   xsd:nonNegativeInteger .

  # Scoping
  # -------------------------------------------------------------------------
  :GenomePosition rdfs:domain :Variant .



## Variant class
## ----------------------------------------------------------------------------
:Variant a owl:Class ;

  edam:has_format   edam:format_3016 ;
  edam:is_output_of edam:operation_3227 .

  # Properties
  # -------------------------------------------------------------------------
  :reference_genome a :Sample ;
     rdfs:label   "Reference genome" ;
     rdfs:comment "The reference genome used to map the sequences to." ;
     rdfs:domain  :Variant ;
     rdfs:range   :Sample .

  :genome_position a :GenomePosition ;
     rdfs:label   "Genome position" ;
     rdfs:comment "The position in the genome when mapped to a reference." ;
     rdfs:domain  :Variant ;
     rdfs:range   :GenomePosition .

  :identifier a owl:DatatypeProperty ;
    rdfs:label   "ID" ;
    rdfs:comment "The ID field of the VCF file." ;
    rdfs:domain  :Variant ;
    rdfs:range   xsd:string .

  :reference a owl:DatatypeProperty ;
    rdfs:label   "Reference" ;
    rdfs:comment "The REF field of the VCF file." ;
    rdfs:domain  :Variant ;
    rdfs:range   xsd:string .

  :alternative a owl:DatatypeProperty ;
    rdfs:label   "Alternative" ;
    rdfs:comment "The ALT field of the VCF file." ;
    rdfs:domain  :Variant ;
    rdfs:range   xsd:string .

  :quality a owl:DatatypeProperty ;
    rdfs:label   "Quality" ;
    rdfs:comment "The QUAL field of the VCF file." ;
    rdfs:domain  :Variant ;
    rdfs:range   xsd:string .

  :filter a owl:DatatypeProperty ;
    rdfs:label   "Filter" ;
    rdfs:comment "The FILTER field of the VCF file." ;
    rdfs:domain  :Variant ;
    rdfs:range   xsd:string .

  :info a owl:DatatypeProperty ;
    rdfs:label   "Info" ;
    rdfs:comment "The INFO field of the VCF file." ;
    rdfs:domain  :Variant ;
    rdfs:range   xsd:string .

  :type a owl:DatatypeProperty ;
    rdfs:label "Type" ;
    rdfs:comment "The SV type determined by the caller." ;
    rdfs:domain :Variant ;
    rdfs:range xsd:string .


## StructuralVariant class
## ----------------------------------------------------------------------------
:StructuralVariant rdf:type owl:Class ;
                   rdfs:subClassOf :Variant .

  # Properties
  # -------------------------------------------------------------------------
  :genome_position2 a :GenomePosition ;
     rdfs:label   "Second genome position" ;
     rdfs:comment "The second position in the genome when mapped to a reference." ;
     rdfs:domain  :StructuralVariant ;
     rdfs:range   :GenomePosition .


## SNPVariant class
## Single-Nucleotide Polymorphism type
## ----------------------------------------------------------------------------
:SNPVariant rdf:type owl:Class ;
            rdfs:subClassOf :Variant .
