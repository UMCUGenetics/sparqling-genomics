@prefix : <http://semweb.op.umcutrecht.nl/smo/> .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix edam: <http://edamontology.org/> .


## Ontology metadata
## ----------------------------------------------------------------------------
<http://semweb.op.umcutrecht.nl/smo/> rdf:type owl:Ontology ;
  owl:versionIRI <http://semweb.op.umcutrecht.nl/1.0.0/smo/> ;
  <http://purl.org/dc/elements/1.1/creator> "Roel Janssen" ;
  <http://purl.org/dc/elements/1.1/title> "Ontology for describing sequence manipulation." .


## Classes
## ----------------------------------------------------------------------------
:SNP
    rdf:type owl:Class ;
    rdfs:comment "Single nucleotide polymorphism"^^xsd:string ;
    rdfs:label "SNP"^^xsd:string .

:Insertion
    rdf:type owl:Class ;
    rdfs:label "Insertion"^^xsd:string .

:Deletion
    rdf:type owl:Class ;
    rdfs:label "Deletion"^^xsd:string .

:Inversion
    rdf:type owl:Class ;
    rdfs:label "Inversion"^^xsd:string .

:Translocation
    rdf:type owl:Class ;
    rdfs:label "Translocation"^^xsd:string .

:Duplication
    rdf:type owl:Class ;
    rdfs:label "Duplication"^^xsd:string .

:TandemDuplication
    rdf:type owl:Class ;
    rdfs:label "TandemDuplication"^^xsd:string ;
    rdfs:subClassOf :Duplication .

:BreakEnd
    rdfs:type owl:Class ;
    rdfs:comment "Arbitrary rearrangement event summarized as a set of novel adjacencies."^^xsd:string ;
    rdfs:label "BreakEnd"^^xsd:string .

:Adjacency
    rdfs:type owl:Class ;
    rdfs:comment "Each adjacency ties together two breakends."^^xsd:string ;
    rdfs:label "Adjacency"^^xsd:string .


## Data types
## ----------------------------------------------------------------------------

:sequence
    rdf:type owl:DatatypeProperty ;
    rdfs:comment ""^^xsd:string ;
    rdfs:label "sequence"^^xsd:string ;
    rdfs:domain :Insertion ;
    rdfs:range [ rdf:type rdfs:Datatype ; owl:onDatatype xsd:integer ; ] .

:length
    rdf:type owl:DatatypeProperty ;
    rdfs:comment ""^^xsd:string ;
    rdfs:label "length"^^xsd:string ;
    rdfs:domain :Insertion ;
    rdfs:range [ rdf:type rdfs:Datatype ; owl:onDatatype xsd:integer ; ] .

:hasMate
    rdf:type owl:ObjectProperty ;
    rdfs:comment "The two breakends connected at either end of a novel adjacency are mates."^^xsd:string ;
    rdfs:domain :BreakEnd ;
    rdfs:label "hasMate" .
