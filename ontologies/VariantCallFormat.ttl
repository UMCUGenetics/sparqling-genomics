@prefix :      <http://semweb.op.umcutrecht.nl/vcf/> .
@prefix faldo: <http://biohackathon.org/resource/faldo#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dc:    <http://purl.org/dc/elements/1.1/> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .

## Ontology metadata
## ----------------------------------------------------------------------------
<http://semweb.op.umcutrecht.nl/vcf/> rdf:type owl:Ontology ;
  dc:creator "Roel Janssen" ;
  dc:title "Ontology for describing records from a VCF file." ;
  owl:versionIRI <http://semweb.op.umcutrecht.nl/2017-11/vcf/> ;
  owl:versionInfo "Created at 2017-11-21 at UMC Utrecht."^^xsd:string .

## Classes
## ----------------------------------------------------------------------------
:Variant
    rdf:type owl:Class ;
    rdfs:label "Variant"^^xsd:string .

:Origin
    rdf:type owl:Class ;
    rdfs:comment "The file from which information originated."^^xsd:string ;
    rdfs:label "Origin"^^xsd:string .

## Data types and object properties
## ----------------------------------------------------------------------------

:filter
    rdf:type xsd:string ;
    rdfs:comment "Arbitrary values added by a variant caller."^^xsd:string ;
    rdfs:label "Filter"^^xsd:string ;
    rdfs:domain :Variant .

:origin
    rdfs:label "Origin"^^xsd:string ;
    rdfs:domain :Variant ;
    rdf:type :Origin .

:start_position
    rdfs:label "Start position"^^xsd:string ;
    rdfs:domain :Variant ;
    rdf:type faldo:ExactPosition .

:end_position
    rdfs:label "End position"^^xsd:string ;
    rdfs:domain :Variant ;
    rdf:type faldo:ExactPosition .

:reference
    rdf:type xsd:string ;
    rdfs:domain :Variant ;
    rdfs:label "Reference"^^xsd:string .

:alternative
    rdf:type xsd:string ;
    rdfs:domain :Variant ;
    rdfs:label "Alternative"^^xsd:string .

:id 
    rdf:type xsd:string ;
    rdfs:domain :Variant ;
    rdfs:label "Identifier"^^xsd:string .

:length
    rdf:type xsd:integer ;
    rdfs:domain :Variant ;
    rdfs:label "Length"^^xsd:string .

:mapping_quality
    rdf:type xsd:integer ;
    rdfs:domain :Variant ;
    rdfs:label "Mapping quality"^^xsd:string .

:quality
    rdf:type xsd:integer ;
    rdfs:domain :Variant ;
    rdfs:label "Quality"^^xsd:string .

:paired_end_support
    rdf:type xsd:integer ;
    rdfs:domain :Variant ;
    rdfs:label "Paired-end support"^^xsd:string .

:confidence_interval_start_position
    rdf:type faldo:InBetweenPosition ;
    rdfs:domain :Variant ;
    rdfs:comment "Confidence interval of start position"^^xsd:string ;
    rdfs:comment "Start position CI"^^xsd:string .

:confidence_interval_end_position
    rdf:type faldo:InBetweenPosition ;
    rdfs:domain :Variant ;
    rdfs:comment "Confidence interval of end position"^^xsd:string ;
    rdfs:comment "End position CI"^^xsd:string .

:sha256_digest
    rdf:type xsd:string ;
    rdfs:comment "The SHA256 hash of the contents of the file."^^xsd:string ;
    rdfs:label "SHA256 digest"^^xsd:string ;
    rdfs:domain :Origin .

:filename
    rdf:type xsd:string ;
    rdfs:comment "Path to the file."^^xsd:string ;
    rdfs:label "Filename"^^xsd:string ;
    rdfs:domain :Origin .

:caller
    rdf:type xsd:string ;
    rdfs:comment "Name and version string for the caller."^^xsd:string ;
    rdfs:label "Caller"^^xsd:string ;
    rdfs:domain :Origin .
